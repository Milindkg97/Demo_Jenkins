pipeline {

  agent any
  stages {
  
    stage("build"){
    
    steps{
        echo 'Building the App...'
        echo 'App was built.'
      
    }  
  }
  stage("test"){
    
    steps{
          echo 'Testing the App...'
    }  
  }
  
  stage("deploy"){
    
    steps{
        echo 'Deploying the App...'
    }  
  }
  
 }
}
